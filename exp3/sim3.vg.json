{
    "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
    "data": {"url": "simulation3.csv"},
    "transform": [
    ],
    "mark": "rect",
    "width": 300,
    "height": 200,
    "encoding": {
      "x": {
        "bin": {"maxbins":60},
        "field": "available_time_h",
        "type": "quantitative"
      },
      "y": {
        "bin": {"maxbins": 40},
        "field": "reviewed_assets",
        "type": "quantitative"
      },
      "color": {
        "aggregate": "count",
        "type": "quantitative"
      }
    },
    "config": {
      "view": {
        "stroke": "transparent"
      }
    }
  }
  